{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "plugin:vue/essential",
  "parserOptions": {
//    "parser": "babel-eslint"
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "arrow-parens": 0,
    "generator-star-spacing": 0,
    "no-unused-vars": "error",
    "indent": [2, 4, { "SwitchCase": 1 }],
    "space-before-function-paren": ["error", "never"]
  }
}
